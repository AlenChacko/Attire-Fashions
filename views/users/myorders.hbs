						  </thead>
						  <tbody>
							{{#each orders}}
						    <tr class="alert" role="alert">
                  <td>
                      <div class="name">
                          <span>{{this.productDetails.name}}</span>
                      </div>
                  </td>
                  <td>   
                    <a href="/track/{{this.productDetails._id}}/{{this._id}}">
                    <div class="img" 
                      style="background-image: url(/product-images/{{this.productDetails._id}}.jpg);">
                    </div></a>
                        
                  </td>
						    	<td>{{this.deliveryDetails.name}}, <br> {{this.deliveryDetails.buildingName}},{{this.deliveryDetails.city}}, <br>{{this.deliveryDetails.pin}}, <br> Phone: {{this.deliveryDetails.phone}}</td>
						       <td>₹ {{this.productDetails.price}}</td>
                   <td>{{this.products.quantity}}</td>
                   <td>{{this.deliveryDetails.totalAmount}}</td>
                   <td>{{this.date}}     </td>
                   <td>{{this.paymentMethod}}</td>
						      	{{!-- <td></td>
								<td></td> --}}
								{{!-- <td>Pending</td> --}}
								{{!-- <td><button class="btn btn-light"><span class="flaticon-visibility"></span></button></td> --}}
								
								
						      
						  
						    </tr>
							{{/each}}
						  </tbody>
						</table>
