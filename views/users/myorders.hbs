    <section class="ftco-section">
    	<div class="container">
    		<div class="row">
    			<div class="table-wrap">
						<table class="table" id="myDiv">
						  <thead class="thead-primary">
						    <tr>
								<th>Product</th>
                <th>View</th>
								<th>Address</th>
						    <th>Price</th>
                <th>Quantity</th>
                <th>Total</th>
                <th>Date</th>
